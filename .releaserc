{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "next",
    {
      "name": "rc",
      "prerelease": true
    },
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "plugins": [
    ["@semantic-release/commit-analyzer", {
      "releaseRules": [
        { "type": "docs", "release": "patch" },
        { "type": "style", "release": "patch" },
        { "type": "refactor", "release": "patch" },
        { "type": "perf", "release": "patch" },
        { "type": "test", "release": "patch" },
        { "type": "build", "release": "patch" },
        { "type": "ci", "release": "patch" },
        { "type": "chore", "release": "patch" },
        { "type": "revert", "release": "patch" }
      ]
    }],
    "@semantic-release/release-notes-generator",
    ["@semantic-release/exec", {
      "publishCmd": "echo 1 && git tag --list && git tag -d v${nextRelease.version} && git tag -s v${nextRelease.version} -m 'susu: v${nextRelease.version}\n\n${nextRelease.notes}'"
    }],
    ["@semantic-release/github", {
      "draft": false
    }],
    "@semantic-release/npm",
    ["@semantic-release/git", {
      "assets": ["package.json", "package-lock.json"],
      "message": "release: v${nextRelease.version}\n\n${nextRelease.notes}"
    }],
    ["@semantic-release/exec", {
      "prepareCmd": "echo 2 && git tag --list && npx clean-package",
      "publishCmd": "echo 3 && git tag --list ",
      "successCmd": "echo 4 && git tag --list && npx clean-package restore"
    }]
  ]
}
